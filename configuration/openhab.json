{
  "fromInterface": {
    "eventUrl": "http://192.168.0.10:8080/rest/events",
    "generalTopicFilter": "openhab\\/items\\/(?<topic>[\\w:]+)\\/statechanged",
    "measurementMappers": [
      {
        "topicFilter": "(?<topic>[\\w:]+(vocht|temp|temp_rfx))",
        "typeFilter": "Quantity",
        "transformer": "temperature",
        "customConfig": {
          "humidityTopicFilter": "vocht",
          "replaceBy": "temp"
        }
      },
      {
        "topicFilter": "vlg_(?<topic>[\\w:]+)",
        "typeFilter": "OnOff",
        "transformer": "switch"
      }
    ]
  },
  "toInterface": {}
}